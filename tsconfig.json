{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "removeComments": true,
        "sourceMap": false,
        "outDir": "dist",
        "baseUrl": ".",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ]
}